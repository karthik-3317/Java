 package stringTokinser;
import java.util.StringTokenizer;
public class Start {
	String text;
	public static void main(String[]args)
	{
		Start st=new Start();
		st.text="my name is karthik venkat sudha sai pavan aditya";
		StringTokenizer stt=new StringTokenizer(st.text," ");
		System.out.println(stt.countTokens());
		while(stt.hasMoreElements())//returns true or false if their is next any elements or not 
		{
			System.out.println(stt.nextToken());
			//it is predefined method it will print next token passed in string tokineser class
		}
	}
}
